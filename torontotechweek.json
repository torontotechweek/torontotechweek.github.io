{
	"version": 8,
	"name": "osm",
	"metadata": {
		"maputnik:renderer": "mbgljs"
	},
	"glyphs": "https://tile-server.mappedin.com/fonts/{fontstack}/{range}.pbf",
	"sprite": "https://tile-server.mappedin.com/styles/mappedin/sprite",
	"sources": {
		"osm": {
			"type": "vector",
			"url": "https://tile-server.mappedin.com/data/osm.json"
		}
	},
	"layers": [
		{
			"id": "background",
			"type": "background",
			"paint": {
				"background-color": "hsl(227, 95%, 95%)"
			}
		},
		{
			"source": "osm",
			"id": "landuse",
			"type": "fill",
			"source-layer": "landuse",
			"filter": [ "in", "class", "residential", "suburb", "neighbourhood", "commercial", "industrial", "garages", "dam", "cemetery", "hospital", "school", "railway" ],
			"paint": {
			  "fill-color": "hsl(227, 95%, 95%)",
			  "fill-opacity": 1
			}
		},
		{
			"source": "osm",
			"id": "landcover-natural",
			"type": "fill",
			"source-layer": "landcover",
			"filter": [ "in", "class", "wood", "grass", "public_park", "sand" ],
				"paint": {
				"fill-color": "hsl(227, 95%, 90%)",
			  	"fill-opacity": 1
			}
		},
		{
			"source": "osm",
			"id": "landcover-natural-outline",
			"type": "line",
			"source-layer": "landcover",
			"filter": [ "in", "class", "wood", "grass", "public_park", "sand" ],
				"paint": {
				"line-color": "hsl(227, 95%, 80%)",
				"line-width": { "stops": [ [ 8, 0.1 ], [ 16, 1 ] ] }
			},
			"minzoom": 8
		},
		{
			"id": "landcover-parking",
			"type": "fill",
			"source": "osm",
			"source-layer": "poi",
			"minzoom": 15,
			"filter": ["==", "subclass", "parking"],
			"paint": {
			  "fill-color": "hsl(227, 95%, 90%)",
			  "fill-opacity": 1
			}
		},
		{
			"id": "boundary",
			"type": "line",
			"source": "osm",
			"source-layer": "boundary",
			"paint": {
				"line-color": "hsl(227, 95%, 80%)",
				"line-width": 1
			}
		},
		{
			"source": "osm",
			"id": "water",
			"type": "fill",
			"source-layer": "water",
			"paint": {
				"fill-color": "hsl(227, 95%, 90%)"
			}
		},
		{
			"source": "osm",
			"id": "waterway",
			"type": "line",
			"source-layer": "waterway",
			"filter": [ "!=", "brunnel", "tunnel" ],
			"paint": {
				"line-color": "hsl(227, 95%, 90%)",
				"line-width": 1,
				"line-opacity": { "stops": [ [ 5, 0 ], [ 6, 1 ] ] }
			}
		},
		{
			"source": "osm",
			"id": "highway-path",
			"type": "line",
			"source-layer": "transportation",
			"filter": [ "all", [ "in", "class", "path" ], [ "!in", "brunnel", "tunnel", "bridge" ] ],
			"paint": {
				"line-color": "hsl(227, 95%, 80%)",
				"line-width": { "stops": [ [ 16, 0.5 ], [ 20, 1.5 ] ] },
				"line-opacity": 1
			},
			"layout": {
				"line-join": "round",
				"line-cap": "round"
			},
			"minzoom": 16
		},
		{
			"source": "osm",
			"id": "building",
			"type": "fill",
			"source-layer": "building",
			"paint": {
				"fill-color": "hsl(227, 95%, 97%)",
				"fill-opacity": { "stops": [ [ 14, 0 ], [ 15, 1 ] ] }
			}
		},
		{
			"source": "osm",
			"id": "building-wall-inside",
			"type": "line",
			"source-layer": "building",
			"paint": {
				"line-color": "hsl(227, 95%, 80%)",
				"line-offset": { "stops": [ [ 15, 2 ], [ 20, 4 ] ] },
				"line-width": { "stops": [ [ 15, 0.5 ], [ 20, 1 ] ] },
				"line-opacity": { "stops": [ [ 15, 0 ], [ 15.5, 1 ] ] }
			},
			"layout": {
				"line-join": "miter",
				"line-cap": "butt"
			}
		},
		{
			"source": "osm",
			"id": "building-wall-outside",
			"type": "line",
			"source-layer": "building",
			"paint": {
				"line-color": "hsl(227, 95%, 80%)",
				"line-width": { "stops": [ [ 15, 0.5 ], [ 20, 1 ] ] },
				"line-opacity": { "stops": [ [ 15, 0 ], [ 15.5, 1 ] ] }
			},
			"layout": {
				"line-join": "miter",
				"line-cap": "butt"
			}
		},
		{
			"id": "ferry",
			"type": "line",
			"source": "osm",
			"source-layer": "transportation",
			"filter": ["in", "class", "ferry"],
			"paint": {
			  "line-color": "hsl(227, 95%, 80%)",
			  "line-dasharray": [2, 2],
			  "line-width": 1
			}
		},
		{
			"source": "osm",
			"id": "aeroway-taxiway",
			"type": "line",
			"source-layer": "aeroway",
			"filter": [ "==", "class", "taxiway" ],
			"paint": {
				"line-color": "hsl(227, 95%, 80%)",
				"line-width": [ "interpolate", [ "exponential", 2 ], [ "zoom" ], 10, 0, 20, 60 ],
				"line-opacity": { "stops": [ [ 13, 0 ], [ 14, 1 ] ] }
			},
			"layout": {
				"line-join": "round"
			}
		},
		{
			"source": "osm",
			"id": "aeroway-runway",
			"type": "line",
			"source-layer": "aeroway",
			"filter": [ "==", "class", "runway" ],
			"paint": {
				"line-color": "hsl(227, 95%, 80%)",
				"line-width": [ "interpolate", [ "exponential", 2 ], [ "zoom" ], 10, 0, 20, 300 ],
				"line-opacity": { "stops": [ [ 11, 0 ], [ 12, 1 ] ] }
			},
			"layout": {
				"line-join": "round"
			}
		},
		{
			"source": "osm",
			"id": "highway-service-outline",
			"type": "line",
			"source-layer": "transportation",
			"filter": [ "all", [ "==", "class", "service" ], [ "!=", "bridge", true ] ],
			"paint": {
				"line-color": "hsl(227, 95%, 80%)",
				"line-width": [ "interpolate", [ "exponential", 2 ], [ "zoom" ], 10, 0, 15, 2, 20, 32 ]
			},
			"layout": {
				"line-join": "round",
				"line-cap": "butt"
			},
			"minzoom": 15
		},
