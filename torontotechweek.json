			},
			"layout": {
				"line-join": "round",
				"line-cap": "round"
			}
		},
		{
			"source": "osm",
			"id": "highway-service",
			"type": "line",
			"source-layer": "transportation",
			"filter": [ "all", [ "==", "class", "service" ], [ "!=", "bridge", true ] ],
			"paint": {
				"line-color": "hsl(227, 95%, 90%)",
				"line-width": [ "interpolate", [ "exponential", 2 ], [ "zoom" ], 10, 0, 20, 30 ]
			},
			"layout": {
				"line-join": "round",
				"line-cap": "round"
			},
			"minzoom": 15
		},
		{
			"source": "osm",
			"id": "highway-minor",
			"type": "line",
			"source-layer": "transportation",
			"filter": [ "in", "class", "minor" ],
			"paint": {
				"line-color": "hsl(227, 95%, 100%)",
				"line-width": [ "interpolate", [ "exponential", 2 ], [ "zoom" ], 10, 0, 20, 92 ],
				"line-opacity": { "stops": [ [ 5, 0 ], [ 6, 1 ] ] }
			},
			"layout": {
				"line-join": "round",
				"line-cap": "round"
			},
			"minzoom": 15
		},
		{
			"source": "osm",
			"id": "highway-major",
			"type": "line",
			"source-layer": "transportation",
			"filter": [ "in", "class", "motorway", "trunk", "primary", "secondary", "tertiary" ],
			"paint": {
				"line-color": "hsl(227, 95%, 100%)",
				"line-width": [ "interpolate", [ "exponential", 2 ], [ "zoom" ], 10, 0, 20, 152 ],
				"line-opacity": { "stops": [ [ 5, 0 ], [ 6, 1 ] ] }
			},
			"layout": {
				"line-join": "round",
				"line-cap": "round"
			}
		},
		{
			"source": "osm",
			"id": "railway-outline",
			"type": "line",
			"source-layer": "transportation",
			"filter": [ "all", [ "in", "class", "rail" ], [ "!has", "service" ], ["!in", "brunnel", "bridge", "tunnel"] ],
			"paint": {
				"line-color": "hsl(227, 95%, 80%)",
				"line-width": [ "interpolate", [ "exponential", 2 ], [ "zoom" ], 10, 2, 20, 14 ],
				"line-opacity": { "stops": [ [ 14, 0 ], [ 15, 1 ] ] }
			}
		},
		{
			"source": "osm",
			"id": "railway",
			"type": "line",
			"source-layer": "transportation",
			"filter": [ "all", [ "in", "class", "rail" ], [ "!has", "service" ], ["!in", "brunnel", "bridge", "tunnel"] ],
			"paint": {
				"line-color": "hsl(227, 95%, 97%)",
				"line-width": [ "interpolate", [ "exponential", 2 ], [ "zoom" ], 10, 1, 20, 10 ],
				"line-dasharray": [ 4, 4 ],
				"line-opacity": { "stops": [ [ 14, 0 ], [ 15, 1 ] ] }
			}
		},
		{
			"source": "osm",
			"id": "highway-minor-name",
			"type": "symbol",
			"source-layer": "transportation_name",
			"filter": [ "in", "class", "minor" ],
			"layout": {
				"text-field": "{name:latin} {name:nonlatin}",
				"text-transform": "uppercase",
				"text-font": [ "lexendzetta-light" ],
				"text-letter-spacing": 0.2,
				"symbol-placement": "line",
				"symbol-spacing": 500,
				"symbol-sort-key": 10,
				"text-anchor": "center",
				"text-size": [ "interpolate", [ "exponential", 1.5 ], [ "zoom" ], 10, 0, 20, 20 ]
			},
			"paint": {
				"text-color": "hsl(227, 95%, 80%)"
			},
			"minzoom": 16
		},
		{
			"source": "osm",
			"id": "highway-major-name",
			"type": "symbol",
			"source-layer": "transportation_name",
			"filter": [ "in", "class", "motorway", "trunk", "primary", "secondary", "tertiary" ],
			"layout": {
				"text-field": "{name:latin} {name:nonlatin}",
				"text-transform": "uppercase",
				"text-font": [ "lexendzetta-light" ],
				"text-letter-spacing": 0.2,
				"symbol-placement": "line",
				"symbol-spacing": 200,
				"symbol-sort-key": 100,
				"text-anchor": "center",
				"text-size": [ "interpolate", [ "exponential", 1.5 ], [ "zoom" ], 10, 0, 20, 40 ]
			},
			"paint": {
				"text-color": "hsl(227, 95%, 80%)"
			}
		},
		{
			"source": "osm",
			"id": "waterway-name",
			"type": "symbol",
			"source-layer": "water_name",
			"filter": [ "in", "class", "river", "stream", "canal", "ditch", "drain" ],
			"layout": {
				"text-field": "{name:latin} {name:nonlatin}",
				"text-transform": "uppercase",
				"text-rotation-alignment": "map",
				"text-font": [ "lexendzetta-light" ],
				"text-letter-spacing": 0,
				"symbol-placement": "line",
				"text-max-angle": 30,
				"text-anchor": "bottom",
				"text-size": { "stops": [ [ 14, 6 ], [ 17, 10 ] ] }
			},
			"paint": {
				"text-color": "hsl(227, 95%, 80%)"
			}
		},
		{
			"source": "osm",
			"id": "water-name",
			"type": "symbol",
			"source-layer": "water_name",
			"filter": [ "!in", "class", "river", "stream", "canal", "ditch", "drain" ],
			"layout": {
				"text-field": "{name:latin}\n{name:nonlatin}",
				"text-transform": "uppercase",
				"text-rotation-alignment": "map",
				"text-font": [ "lexendzetta-light" ],
				"text-letter-spacing": 0,
				"text-line-height": 0.9,
				"text-max-width": 3,
				"symbol-placement": "point",
				"text-anchor": "center",
				"text-size": { "stops": [ [ 14, 6 ], [ 17, 10 ] ] }
			},
			"paint": {
				"text-color": "hsl(227, 95%, 80%)"
			}
		},
		{
			"source": "osm",
			"id": "place-city",
			"type": "symbol",
			"source-layer": "place",
			"filter": [ "in", "class", "city", "town", "village" ],
			"layout": {
				"text-field": "{name:latin}\n{name:nonlatin}",
				"text-transform": "uppercase",
				"text-font": [ "montserrat-light" ],
				"text-letter-spacing": 0.1,
				"text-size": { "stops": [ [ 5, 8 ], [ 8, 10 ] ] }
			},
			"paint": {
				"text-color": "hsl(227, 95%, 50%)",
				"text-halo-color": "hsl(227, 95%, 95%)",
		        "text-halo-width": 2
			}
		},
		{
			"source": "osm",
			"id": "place-state",
			"type": "symbol",
			"source-layer": "place",
			"filter": [ "in", "class", "state" ],
			"layout": {
				"text-field": "{name:latin}\n{name:nonlatin}",
				"text-transform": "uppercase",
				"text-font": [ "lexendzetta-light" ],
				"text-letter-spacing": 0,
