				"text-opacity": [ "interpolate", [ "linear" ], [ "zoom" ], 13, 0, 14, 1 ],
				"icon-color": "hsl(52, 96%, 66%)",
				"text-color": "hsl(52, 96%, 66%)",
				"text-halo-color": "hsl(0, 0%, 100%)",
				"text-halo-width": 0,
