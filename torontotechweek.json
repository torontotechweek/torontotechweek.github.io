				"line-color": "hsl(227, 95%, 80%)",
				"line-width": [ "interpolate", [ "exponential", 2 ], [ "zoom" ], 10, 0, 15, 4, 20, 96 ],
				"line-opacity": { "stops": [ [ 5, 0 ], [ 6, 1 ] ] }
			},
			"layout": {
				"line-join": "round",
				"line-cap": "round"
			},
			"minzoom": 15
		},
		{
			"source": "osm",
			"id": "highway-major-outline",
			"type": "line",
			"source-layer": "transportation",
			"filter": [ "in", "class", "motorway", "trunk", "primary", "secondary", "tertiary" ],
			"paint": {
				"line-color": "hsl(227, 95%, 80%)",
				"line-width": [ "interpolate", [ "exponential", 2 ], [ "zoom" ], 10, 0, 15, 6, 20, 156 ],
